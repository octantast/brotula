            if (fishpossibility <= 5)
            {
                if (fishpossibility == -5)
                {
                    randomPossibility = Random.Range(1, 100);
                    spawnPosition = new Vector3(13.4f, Random.Range(-1140f, -1100f), -788f);
                }
                else // что это?
                {
                    spawnPosition = new Vector3(13.4f, Random.Range(-1140f, -1100f), -788f);
                    randomPossibility = Random.Range(1, 115);
                }
                if (gens.pEcholocatia >= 2 && gens.pLipid < 2)
                {
                    if (randomPossibility > 102)
                    {
                        randomPossibility = Random.Range(1, 115);
                    }
                    if (randomPossibility > 102 && gens.pEcholocatia == 3)
                    {
                        randomPossibility = Random.Range(1, 115);
                    }
                }


                if (randomPossibility == 1) { Instantiate(Paster, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 2) { Instantiate(Sciph, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 3) { Instantiate(Sactin, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 4) { Instantiate(Zem, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 5) { Instantiate(Thanaid, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 6) { Instantiate(Nemin, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 6 && randomPossibility <= 8) { Instantiate(Ekhir, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 8 && randomPossibility <= 10) { Instantiate(Tkhar, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 12 && randomPossibility <= 14) { Instantiate(Galan, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 14 && randomPossibility <= 31) { Instantiate(Demos, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 31 && randomPossibility <= 48) { Instantiate(Nerrelid, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 48 && randomPossibility <= 66) { Instantiate(Khirond, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 66 && randomPossibility <= 84) { Instantiate(Golor, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 84 && randomPossibility <= 102) { Instantiate(Marius, spawnPosition, Quaternion.identity, enemyholder.transform); }
            }
            spawned15 = true;
        }

        // 11500-12000
        if (spawned16 == false && spawned15)
        {
            if (fishpossibility <= 5)
            {
                if (fishpossibility == -5)
                {
                    randomPossibility = Random.Range(1, 100);
                    spawnPosition = new Vector3(13.4f, Random.Range(-1170f, -1160f), -788f);
                }
                else // что это?
                {
                    spawnPosition = new Vector3(13.4f, Random.Range(-1170f, -1160f), -788f);
                    randomPossibility = Random.Range(1, 115);
                }

                if (gens.pEcholocatia >= 2 && gens.pLipid < 2)
                {
                    if (randomPossibility > 102)
                    {
                        randomPossibility = Random.Range(1, 115);
                    }
                    if (randomPossibility > 102 && gens.pEcholocatia == 3)
                    {
                        randomPossibility = Random.Range(1, 115);
                    }
                }

                if (randomPossibility == 1) { Instantiate(Paster, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 2) { Instantiate(Sciph, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 3) { Instantiate(Sactin, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 4) { Instantiate(Zem, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 5) { Instantiate(Thanaid, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 6) { Instantiate(Nemin, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 6 && randomPossibility <= 8) { Instantiate(Ekhir, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 8 && randomPossibility <= 10) { Instantiate(Tkhar, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 12 && randomPossibility <= 14) { Instantiate(Galan, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 14 && randomPossibility <= 31) { Instantiate(Demos, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 31 && randomPossibility <= 48) { Instantiate(Nerrelid, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 48 && randomPossibility <= 66) { Instantiate(Khirond, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 66 && randomPossibility <= 84) { Instantiate(Golor, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 84 && randomPossibility <= 102) { Instantiate(Marius, spawnPosition, Quaternion.identity, enemyholder.transform); }
            }
            spawned16 = true;
        }

        // 12000 - 12500
        if (spawned17 == false && spawned16)
        {
            if (fishpossibility <= 5)
            {
                if (fishpossibility == -5)
                {
                    randomPossibility = Random.Range(1, 100);
                    spawnPosition = new Vector3(13.4f, Random.Range(-1250f, -1240f), -788f);
                }
                else // что это?
                {
                    spawnPosition = new Vector3(13.4f, Random.Range(-1250f, -1240f), -788f);
                    randomPossibility = Random.Range(1, 115);
                }

                if (gens.pEcholocatia >= 2 && gens.pLipid < 2)
                {
                    if (randomPossibility > 102)
                    {
                        randomPossibility = Random.Range(1, 115);
                    }
                    if (randomPossibility > 102 && gens.pEcholocatia == 3)
                    {
                        randomPossibility = Random.Range(1, 115);
                    }
                }
                if (randomPossibility == 1) { Instantiate(Paster, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 2) { Instantiate(Sciph, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 3) { Instantiate(Sactin, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 4) { Instantiate(Zem, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 5) { Instantiate(Thanaid, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility == 6) { Instantiate(Nemin, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 6 && randomPossibility <= 8) { Instantiate(Ekhir, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 8 && randomPossibility <= 10) { Instantiate(Tkhar, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 12 && randomPossibility <= 14) { Instantiate(Galan, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 14 && randomPossibility <= 31) { Instantiate(Demos, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 31 && randomPossibility <= 48) { Instantiate(Nerrelid, spawnPosition, Quaternion.identity, enemyholder.transform); }
                else if (randomPossibility > 48 && randomPossibility <= 66) { Instantiate(Khirond, spawnPosition, Quaternion.identity, enemyholder.transform); }
