                        if (mermPosition2justset)
                        {
                            mermaids.herPosition2 = 8;
                            Instantiate(Benta, mermaidslot2.transform.position, Quaternion.identity, mermaidslot2.transform);
                        }
                        if (mermPosition3justset)
                        {
                            mermaids.herPosition3 = 8;
                            Instantiate(Benta, mermaidslot3.transform.position, Quaternion.identity, mermaidslot3.transform);
