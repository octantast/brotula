    void Start()
    {
        timerdeath = 5f;
        resultMode = false;
        presentIkra.SetActive(false);
        garemSpecial2Button.SetActive(false);
        zametkascreen.SetActive(false);
    }

    void Update()
    {

        // Здесь игрок соглашается с ничьей / проигрышем, происходят последствия
        //if (resultMode == true && podtverjdenie.podtverjdenoOk == true && podtverjdenie.presentBack.activeSelf == false)
        //{
            //if (player.shkalaUzhasaVraga > 0)
            //{
               // forLevel.expe += 2;
            //}
            //if (player.shkalaUzhasaGG >= player.shkalaUzhasaVraga || respawnringwo != 0)
            //{
// тут было подтверждение поражения
           // }
        //}

        if (resultMode == true && podtverjdenie.podtverjdenoOk == true)
        {
            podtverjdenie.podtverjdenoOk = false;
            podtverjdenie.podtverjdenoTapom = false;
            podtverjdenie.noteBack.SetActive(false);
            podtverjdenie.noteText.text = " ";
            podtverjdenie.confirmOk.SetActive(false);
        }


        // Здесь игрок соглашается с победой, происходят последствия




        if ((resultMode == true && podtverjdenie.podtverjdenoTapom == true && podtverjdenie.podtverjdenoOk == false && mermaids.confirmGaremBack.activeSelf == false) || (playerControl.nagradoid == true && podtverjdenie.podtverjdenoTapom == true && podtverjdenie.podtverjdenoOk == false && mermaids.confirmGaremBack.activeSelf == false))
        {

            if ((player.shkalaUzhasaGG < player.shkalaUzhasaVraga || playerControl.nagradoid == true) && holders.RingwooditKey == false)
            {
                if (holders.IkraBonusKey == true) // если игрок выбрал икру
                {
                    ikorka.ikra += battleModeVisibility.randomIkornyy;
                    battleModeVisibility.newMermaids = 0;
                    forLevel.expe += 4;
                    PlayerPrefs.SetFloat("expe", forLevel.expe);
                    PlayerPrefs.SetFloat("level", forLevel.level);
                    if (distanceCount.distance > forLevel.maxDepth)
                    {
                        PlayerPrefs.SetFloat("maxDepth", distanceCount.distance);
                    }
                    PlayerPrefs.Save();
                    mermaids.mermaidButtonImitator.SetActive(false);
                    presentIkra.SetActive(false);
                    bonusikraText.text = " ";
                    podtverjdenie.noteBack.SetActive(false);
                    player.playingMode = false;
                    resultMode = false;
                    nagradataker = false;
                    if(enemySpawner.fishbiom)
                    {
                        mermanfish.SetActive(false);
                    }

                    battleModeVisibility.skippingMode = true;

                    // Восстановление, ПРОВЕРКА
                }
                if (holders.ZametkaKey == true) // если игрок выбрал заметку
                {
                    battleModeVisibility.mermaidprevious = battleModeVisibility.mermaidPlace2;
                    battleModeVisibility.newMermaids = 0;
                    forLevel.expe += 4;
                    PlayerPrefs.SetFloat("expe", forLevel.expe);
                    PlayerPrefs.SetFloat("level", forLevel.level);
                    if (distanceCount.distance > forLevel.maxDepth)
                    {
                        PlayerPrefs.SetFloat("maxDepth", distanceCount.distance);
                    }
                    battleModeVisibility.zapiski += 1;
                    PlayerPrefs.SetFloat("zapiski", battleModeVisibility.zapiski);
                    PlayerPrefs.Save();
                    mermaids.mermaidButtonImitator.SetActive(false);
                    presentIkra.SetActive(false);
                    bonusikraText.text = " ";
                    podtverjdenie.noteBack.SetActive(false);
                    player.playingMode = false;
                    resultMode = false;
                    nagradataker = false;
                    // включение заметки, и оно должно блокировать награду уровня, и потом выключиться
                    battleModeVisibility.zapisoid();
                    zametkascreen.SetActive(true);
                    battleModeVisibility.skippingMode = true;
                }

                if (mermaids.mermaids + battleModeVisibility.newMermaids > forLevel.garemSize && battleModeVisibility.newMermaids == 1) // если лимит русалок превышен, и игрок выбрал новую русалку
                {
                    // убрать ключ, подсветку и поисание, вывести подсказку, которая исчезает с тапом в любом месте
                    holders.obnulenie_priza_batol();
                    obuchregul.podsklimit.SetActive(true);
                }
                if (mermaids.mermaids + battleModeVisibility.newMermaids <= forLevel.garemSize && battleModeVisibility.newMermaids == 1) // если лимит русалок не превышен, и игрок выбрал новую русалку
                {
                    evermermaids = 1;
                    battleModeVisibility.newMermaids = 0;
                    mermaids.mermaids += 1;
                    // определяем свободную позицию
                    if (mermaids.mermPosition1 == false)
                    {
                        mermaids.mermPosition1 = true;
                        mermPosition1justset = true;
                    }
                    if(mermPosition1justset == false && mermaids.mermPosition2 == false)
                    {
                        mermaids.mermPosition2 = true;
                        mermPosition2justset = true;
                    }
                    if (mermPosition1justset == false && mermPosition2justset == false && mermaids.mermPosition3 == false)
                    {
                        mermaids.mermPosition3 = true;
                        mermPosition3justset = true;
                    }
                    if (mermPosition1justset == false && mermPosition2justset == false && mermPosition3justset == false && mermaids.mermPosition4 == false)
                    {
                        mermaids.mermPosition4 = true;
                        mermPosition4justset = true;
                    }
                    if (mermPosition1justset == false && mermPosition2justset == false && mermPosition3justset == false && mermPosition4justset == false && mermaids.mermPosition5 == false)
                    {
                        mermaids.mermPosition5 = true;
                        mermPosition5justset = true;
                    }
                    if (mermPosition1justset == false && mermPosition2justset == false && mermPosition3justset == false && mermPosition4justset == false && mermPosition5justset == false && mermaids.mermPosition6 == false)
                    {
                        mermaids.mermPosition6 = true;
                        mermPosition6justset = true;
                    }









                    if (holders.KrideiaMermaidKey)
                    {
                        PlayerPrefs.SetFloat("pKrideiaMermaid", 1);
                        PlayerPrefs.Save();
                        if (mermPosition1justset)
                        {
                            mermaids.herPosition1 = 1;
                            Instantiate(Krideia, mermaidslot1.transform.position, Quaternion.identity, mermaidslot1.transform);
                        }
                        if (mermPosition2justset)
                        {
                            mermaids.herPosition2 = 1;
                            Instantiate(Krideia, mermaidslot2.transform.position, Quaternion.identity, mermaidslot2.transform);
                        }
                        if (mermPosition3justset)
                        {
                            mermaids.herPosition3 = 1;
                            Instantiate(Krideia, mermaidslot3.transform.position, Quaternion.identity, mermaidslot3.transform);
                        }
                        if (mermPosition4justset)
                        {
                            mermaids.herPosition4 = 1;
                            Instantiate(Krideia, mermaidslot4.transform.position, Quaternion.identity, mermaidslot4.transform);
                        }
