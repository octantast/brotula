                    {
                        PlayerPrefs.SetFloat("pBatizaMermaid", 1);
                        PlayerPrefs.Save();
                        if (mermPosition1justset)
                        {
                            mermaids.herPosition1 = 2;
                            Instantiate(Batiza, mermaidslot1.transform.position, Quaternion.identity, mermaidslot1.transform);
                        }
