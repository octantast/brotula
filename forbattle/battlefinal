                            Instantiate(Khironda, mermaidslot2.transform.position, Quaternion.identity, mermaidslot2.transform);
                        }
                        if (mermPosition3justset)
                        {
                            mermaids.herPosition3 = 44;
                            Instantiate(Khironda, mermaidslot3.transform.position, Quaternion.identity, mermaidslot3.transform);
                        }
                        if (mermPosition4justset)
                        {
                            mermaids.herPosition4 = 44;
                            Instantiate(Khironda, mermaidslot4.transform.position, Quaternion.identity, mermaidslot4.transform);
                        }
                        if (mermPosition5justset)
                        {
                            mermaids.herPosition5 = 44;
                            Instantiate(Khironda, mermaidslot5.transform.position, Quaternion.identity, mermaidslot5.transform);
                        }
                        if (mermPosition6justset)
