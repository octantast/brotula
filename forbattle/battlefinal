                        }
                        if (mermPosition3justset)
                        {
                            mermaids.herPosition3 = 45;
                            Instantiate(Hadeea, mermaidslot3.transform.position, Quaternion.identity, mermaidslot3.transform);
                        }
                        if (mermPosition4justset)
                        {
                            mermaids.herPosition4 = 45;
                            Instantiate(Hadeea, mermaidslot4.transform.position, Quaternion.identity, mermaidslot4.transform);
                        }
                        if (mermPosition5justset)
                        {
                            mermaids.herPosition5 = 45;
                            Instantiate(Hadeea, mermaidslot5.transform.position, Quaternion.identity, mermaidslot5.transform);
                        }
                        if (mermPosition6justset)
                        {
                            mermaids.herPosition6 = 45;
                            Instantiate(Hadeea, mermaidslot6.transform.position, Quaternion.identity, mermaidslot6.transform);
                        }
                    }

