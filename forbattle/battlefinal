                        }
                        if (mermPosition2justset)
                        {
                            mermaids.herPosition2 = 6;
                            Instantiate(Typhlona, mermaidslot2.transform.position, Quaternion.identity, mermaidslot2.transform);
                        }
                        if (mermPosition3justset)
                        {
                            mermaids.herPosition3 = 6;
                            Instantiate(Typhlona, mermaidslot3.transform.position, Quaternion.identity, mermaidslot3.transform);
                        }
                        if (mermPosition4justset)
                        {
                            mermaids.herPosition4 = 6;
                            Instantiate(Typhlona, mermaidslot4.transform.position, Quaternion.identity, mermaidslot4.transform);
                        }
                        if (mermPosition5justset)
                        {
                            mermaids.herPosition5 = 6;
                            Instantiate(Typhlona, mermaidslot5.transform.position, Quaternion.identity, mermaidslot5.transform);
                        }
                        if (mermPosition6justset)
                        {
                            mermaids.herPosition6 = 6;
                            Instantiate(Typhlona, mermaidslot6.transform.position, Quaternion.identity, mermaidslot6.transform);
                        }
                    }

                    if (holders.NeminaMermaidKey)
                    {
                        PlayerPrefs.SetFloat("pNeminaMermaid", 1);
                        PlayerPrefs.Save();
                        if (mermPosition1justset)
                        {
                            mermaids.herPosition1 = 7;
                            Instantiate(Nemina, mermaidslot1.transform.position, Quaternion.identity, mermaidslot1.transform);
