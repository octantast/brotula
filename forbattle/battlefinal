                    mermPosition2justset = false;
                    mermPosition3justset = false;
                    mermPosition4justset = false;
                    mermPosition5justset = false;
                    mermPosition6justset = false;
                    forLevel.expe += 4;
                    PlayerPrefs.SetFloat("expe", forLevel.expe);
                    PlayerPrefs.SetFloat("level", forLevel.level);
                    if (distanceCount.distance > forLevel.maxDepth)
                    {
                        PlayerPrefs.SetFloat("maxDepth", distanceCount.distance);
                    }
                    PlayerPrefs.Save();
                    mermaids.mermaidButtonImitator.SetActive(false);
                    presentIkra.SetActive(false);
                    bonusikraText.text = " ";
                    podtverjdenie.noteBack.SetActive(false);
                    player.playingMode = false;
                    resultMode = false;
                    nagradataker = false;
                    battleModeVisibility.skippingMode = true;

                    // Восстановление, ПРОВЕРКА
                }

            }






        }


        

        // Здесь выдаётся результат боя

        if (resultMode == true && podtverjdenie.podtverjdenoOk == false && podtverjdenie.noteBack.activeSelf == false && podtverjdenie.presentBack.activeSelf == false)
        {
            slotsmerman.slo1blocked.SetActive(false);
            slotsmerman.slo2blocked.SetActive(false);
            slotsmerman.slo3blocked.SetActive(false);
            slotsmerman.slo4blocked.SetActive(false);
            slotsmerman.slo5blocked.SetActive(false);
            slotsmerman.slo6blocked.SetActive(false);
            slotsmerman.slo7blocked.SetActive(false);
            slotsmerman.slo8blocked.SetActive(false);
            slotsmerman.slo9blocked.SetActive(false);

            if (enemySpawner.dot1 || enemySpawner.dot2 || enemySpawner.dot3)
            {
                // победа
                if (enemySpawner.dot1 && slotsmerman.slo2Hp <= 0 && slotsmerman.slo4Hp <= 0 && slotsmerman.slo6Hp <= 0 && slotsmerman.slo8Hp <= 0 && slotsmerman.slo1Hp > 0 && slotsmerman.slo3Hp > 0 && slotsmerman.slo5Hp > 0 && slotsmerman.slo7Hp > 0 && slotsmerman.slo9Hp > 0)
                {
                    kvest1.Play(0);
                    pobedoid();
                }
                if (enemySpawner.dot2 && slotsmerman.slo2Hp <= 0 && slotsmerman.slo6Hp <= 0 && slotsmerman.slo8Hp <= 0 && slotsmerman.slo4Hp > 0 && slotsmerman.slo1Hp > 0 && slotsmerman.slo3Hp > 0 && slotsmerman.slo5Hp > 0 && slotsmerman.slo7Hp > 0 && slotsmerman.slo9Hp > 0)
                {
                    kvest2.Play(0);
                    pobedoid();
                }
                if (enemySpawner.dot3 && slotsmerman.slo2Hp <= 0 && slotsmerman.slo6Hp > 0 && slotsmerman.slo8Hp <= 0 && slotsmerman.slo4Hp > 0 && slotsmerman.slo1Hp <= 0 && slotsmerman.slo3Hp <= 0 && slotsmerman.slo5Hp <= 0 && slotsmerman.slo7Hp > 0 && slotsmerman.slo9Hp > 0)
                {
                    pobedoid();
                }

                if (enemySpawner.dot1 && (slotsmerman.slo2Hp > 0 || slotsmerman.slo4Hp > 0 || slotsmerman.slo6Hp > 0 || slotsmerman.slo8Hp > 0 || slotsmerman.slo1Hp <= 0 || slotsmerman.slo3Hp <= 0 || slotsmerman.slo5Hp <= 0 || slotsmerman.slo7Hp <= 0 || slotsmerman.slo9Hp <= 0))
                {

                    failringwo();
                }
                if (enemySpawner.dot2 && (slotsmerman.slo2Hp > 0 || slotsmerman.slo6Hp > 0 || slotsmerman.slo8Hp > 0 || slotsmerman.slo4Hp <= 0 || slotsmerman.slo1Hp <= 0 || slotsmerman.slo3Hp <= 0 || slotsmerman.slo5Hp <= 0 || slotsmerman.slo7Hp <= 0 || slotsmerman.slo9Hp <= 0))
                {
                    failringwo();
                }
                if (enemySpawner.dot3 && (slotsmerman.slo2Hp > 0 || slotsmerman.slo6Hp <= 0 || slotsmerman.slo8Hp > 0 || slotsmerman.slo4Hp <= 0 || slotsmerman.slo1Hp > 0 || slotsmerman.slo3Hp > 0 || slotsmerman.slo5Hp > 0 || slotsmerman.slo7Hp <= 0 || slotsmerman.slo9Hp <= 0))
                {
                    failringwo();
                }
            }
            else
            {
                // это если обычный бой
                if (player.shkalaUzhasaGG < player.shkalaUzhasaVraga) // победа
                {
                    // если активирован какой-то приз
                    mermaids.mermaidButtonImitator.SetActive(true);
                    if (forLevel.languager == 0)
                    {
                        podtverjdenie.presentText.text = "Win";
                    }
                    if (forLevel.languager == 1)
                    {
                        podtverjdenie.presentText.text = "Победа";
                    }
                    else if (forLevel.languager == 2)
                    {
                        podtverjdenie.presentText.text = "Перемога";
                    }
                    if (forLevel.languager == 0)
                    {
                        podtverjdenie.presentText3.text = "Choose a reward";
                    }
                    if (forLevel.languager == 1)
                    {
                        podtverjdenie.presentText3.text = "Пора выбирать награду";
                    }
                    else if (forLevel.languager == 2)
                    {
                        podtverjdenie.presentText3.text = "Час обирати нагороду";
                    }
                    nagradataker = true;
                    podtverjdenie.podtverjdenoTapom = false;
                    podtverjdenie.presentBack.SetActive(true);
                    if (battleModeVisibility.mermaidPlace2 != 100 || battleModeVisibility.randomIkornyy != 0) // если не рингвудит и вообще есть икра
                    {
                        presentIkra.SetActive(true);
                        bonusikraText.text = battleModeVisibility.randomIkornyy.ToString("F0");
                    }

                    // спавн значка русалки зависимо от её вида
                    if (battleModeVisibility.mermaidPlace1 == 2 || battleModeVisibility.mermaidPlace1 == 4 || battleModeVisibility.mermaidPlace1 == 5 || battleModeVisibility.mermaidPlace1 == 6 || battleModeVisibility.mermaidPlace1 == 11 || battleModeVisibility.mermaidPlace1 == 16 || battleModeVisibility.mermaidPlace1 == 20 || battleModeVisibility.mermaidPlace1 == 25 || battleModeVisibility.mermaidPlace1 == 27 || battleModeVisibility.mermaidPlace1 == 31 || battleModeVisibility.mermaidPlace1 == 32 || battleModeVisibility.mermaidPlace1 == 33 || battleModeVisibility.mermaidPlace1 == 35) // рыбы
                    {
                        Instantiate(presentryba, place1.transform.position, Quaternion.identity, place1.transform);
                    }
                    else if (battleModeVisibility.mermaidPlace1 == 1 || battleModeVisibility.mermaidPlace1 == 9 || battleModeVisibility.mermaidPlace1 == 37 || battleModeVisibility.mermaidPlace1 == 42 || battleModeVisibility.mermaidPlace1 == 44 || battleModeVisibility.mermaidPlace1 == 47)
                    {
                        Instantiate(presentrak, place1.transform.position, Quaternion.identity, place1.transform);
                    }
                    else if (battleModeVisibility.mermaidPlace1 == 3 || battleModeVisibility.mermaidPlace1 == 15 || battleModeVisibility.mermaidPlace1 == 19 || battleModeVisibility.mermaidPlace1 == 23 || battleModeVisibility.mermaidPlace1 == 38)
                    {
                        Instantiate(presentmolusk, place1.transform.position, Quaternion.identity, place1.transform);
                    }
                    else if (battleModeVisibility.mermaidPlace1 == 7 || battleModeVisibility.mermaidPlace1 == 14 || battleModeVisibility.mermaidPlace1 == 24 || battleModeVisibility.mermaidPlace1 == 41 || battleModeVisibility.mermaidPlace1 == 46)
                    {
                        Instantiate(presentcherv, place1.transform.position, Quaternion.identity, place1.transform);
                    }
                    else if (battleModeVisibility.mermaidPlace1 == 8 || battleModeVisibility.mermaidPlace1 == 21 || battleModeVisibility.mermaidPlace1 == 10)
                    {
                        Instantiate(presentzvezdi, place1.transform.position, Quaternion.identity, place1.transform);
                    }
                    else if (battleModeVisibility.mermaidPlace1 == 12 || battleModeVisibility.mermaidPlace1 == 17 || battleModeVisibility.mermaidPlace1 == 22 || battleModeVisibility.mermaidPlace1 == 28 || battleModeVisibility.mermaidPlace1 == 29 || battleModeVisibility.mermaidPlace1 == 34 || battleModeVisibility.mermaidPlace1 == 36 || battleModeVisibility.mermaidPlace1 == 40)
                    {
                        Instantiate(presentmedus, place1.transform.position, Quaternion.identity, place1.transform);
                    }
                    else if (battleModeVisibility.mermaidPlace1 == 13 || battleModeVisibility.mermaidPlace1 == 18 || battleModeVisibility.mermaidPlace1 == 48 || battleModeVisibility.mermaidPlace1 == 30)
                    {
                        Instantiate(presenthubka, place1.transform.position, Quaternion.identity, place1.transform);
                    }

                    if (battleModeVisibility.randomObilia == 2)
                    {
                        if (battleModeVisibility.mermaidPlace2 == 2 || battleModeVisibility.mermaidPlace2 == 4 || battleModeVisibility.mermaidPlace2 == 5 || battleModeVisibility.mermaidPlace2 == 6 || battleModeVisibility.mermaidPlace2 == 11 || battleModeVisibility.mermaidPlace2 == 16 || battleModeVisibility.mermaidPlace2 == 20 || battleModeVisibility.mermaidPlace2 == 25 || battleModeVisibility.mermaidPlace2 == 27 || battleModeVisibility.mermaidPlace2 == 31 || battleModeVisibility.mermaidPlace2 == 32 || battleModeVisibility.mermaidPlace2 == 33 || battleModeVisibility.mermaidPlace2 == 35) // рыбы
                        {
                            Instantiate(presentryba, place2.transform.position, Quaternion.identity, place2.transform);
                        }
                        else if (battleModeVisibility.mermaidPlace2 == 1 || battleModeVisibility.mermaidPlace2 == 9 || battleModeVisibility.mermaidPlace2 == 37 || battleModeVisibility.mermaidPlace2 == 42 || battleModeVisibility.mermaidPlace2 == 44 || battleModeVisibility.mermaidPlace2 == 47)
                        {
                            Instantiate(presentrak, place2.transform.position, Quaternion.identity, place2.transform);
                        }
                        else if (battleModeVisibility.mermaidPlace2 == 3 || battleModeVisibility.mermaidPlace2 == 15 || battleModeVisibility.mermaidPlace2 == 19 || battleModeVisibility.mermaidPlace2 == 23 || battleModeVisibility.mermaidPlace2 == 38)
                        {
                            Instantiate(presentmolusk, place2.transform.position, Quaternion.identity, place2.transform);
                        }
                        else if (battleModeVisibility.mermaidPlace2 == 7 || battleModeVisibility.mermaidPlace2 == 14 || battleModeVisibility.mermaidPlace2 == 24 || battleModeVisibility.mermaidPlace2 == 41 || battleModeVisibility.mermaidPlace2 == 46)
                        {
                            Instantiate(presentcherv, place2.transform.position, Quaternion.identity, place2.transform);
                        }
                        else if (battleModeVisibility.mermaidPlace2 == 8 || battleModeVisibility.mermaidPlace2 == 21 || battleModeVisibility.mermaidPlace2 == 10)
                        {
                            Instantiate(presentzvezdi, place2.transform.position, Quaternion.identity, place2.transform);
                        }
                        else if (battleModeVisibility.mermaidPlace2 == 12 || battleModeVisibility.mermaidPlace2 == 17 || battleModeVisibility.mermaidPlace2 == 22 || battleModeVisibility.mermaidPlace2 == 28 || battleModeVisibility.mermaidPlace2 == 29 || battleModeVisibility.mermaidPlace2 == 34 || battleModeVisibility.mermaidPlace2 == 36 || battleModeVisibility.mermaidPlace2 == 40)
                        {
                            Instantiate(presentmedus, place2.transform.position, Quaternion.identity, place2.transform);
                        }
                        else if (battleModeVisibility.mermaidPlace2 == 13 || battleModeVisibility.mermaidPlace2 == 18 || battleModeVisibility.mermaidPlace2 == 48 || battleModeVisibility.mermaidPlace2 == 30)
                        {
                            Instantiate(presenthubka, place2.transform.position, Quaternion.identity, place2.transform);
                        }
                        else if (battleModeVisibility.mermaidPlace2 == 60)
                        {
                            Instantiate(presentnote, place2.transform.position, Quaternion.identity, place2.transform);
                        }
                        else if (battleModeVisibility.mermaidPlace2 == 100)
                        {
                            Instantiate(ringvudit, place2.transform.position, Quaternion.identity, place2.transform);
                        }

                    }
                    garemSpecial2Button.SetActive(true);

                }

                else if (player.shkalaUzhasaGG >= player.shkalaUzhasaVraga && moskcolorchange == false) // поражение
                {
                    moskcolorchange = true;
                    player.playingMode = false;
                    resultMode = false;
                    nagradataker = false;
                    // тут мб предложение рекламы

                    secondchanse += 1;
                    forLevel.expe += 1;
                    PlayerPrefs.SetFloat("expe", forLevel.expe);
                    PlayerPrefs.SetFloat("level", forLevel.level);
                    PlayerPrefs.SetFloat("evermermaids", evermermaids);

                    if (distanceCount.distance > forLevel.maxDepth)
                    {
                        PlayerPrefs.SetFloat("maxDepth", distanceCount.distance);
                    }
                    PlayerPrefs.Save();
                    if (secondchanse > 1)
                    {
                        deathmode = true;
                    }
                    else
                    {
                        podtverjdenie.podtverjdenoOk = false;
                        podtverjdenie.noteBack.SetActive(true);
                    }

                }
            }

        }

        if(moskcolorchange)
        {

            //galochka.color = Color.Lerp(galochka.color, transparent, Time.deltaTime / 2f); 
            mosk.color = Color.Lerp(mosk.color, gnilcolor, Time.deltaTime / 1f);
            timerdeath -= Time.deltaTime;
            if (forLevel.languager == 0)
            {
                podtverjdenie.noteText.text = "intimidation failed. \ndistract the enemy by\nvision broadcast? (" + timerdeath.ToString("F0") + ")";
            }
            if (forLevel.languager == 1)
            {
                podtverjdenie.noteText.text = "запугать не вышло. \nнаслать на врага видения? \n(" + timerdeath.ToString("F0") + ")";
            }
            else if (forLevel.languager == 2)
            {
                podtverjdenie.noteText.text = "залякування марне. \nнаслати на ворога ілюзії? \n(" + timerdeath.ToString("F0") + ")";
            }
            if (timerdeath <= 0)
            {
                podtverjdenie.noteBack.SetActive(false);
                podtverjdenie.noteText.text = " ";
                podtverjdenie.confirmOk.SetActive(false);
                deathmode = true;
            }
        }
    }

    public void failringwo()
    {
        if (respawnringwo == 0) //поражение первое с респавном, тут гг должен покрыться коркой со звуком, без оповещения
        {
            respawnringwo = 1f;
            kristl.Play(0);
            // тут корка, плавнее, смотри на мимикрию
            anime.ringwohead.SetActive(true);
            anime.ringwoarm.SetActive(true);
            anime.ringwodown.SetActive(true);

            // респавн и открыть навыки заново?
            player.playingMode = false;
            resultMode = false;
            nagradataker = false;
            mermanreader.skipEnemy(); // очистка после боя
            battleModeVisibility.randomObilia = 2;
            battleModeVisibility.mermaidPlace1 = 0;
            battleModeVisibility.mermaidPlace2 = 100;
            battleModeVisibility.randomIkornyy = 0;
            battleModeVisibility.mermanChitaetsya = true; // спавн всего заново


        }
        else if (respawnringwo != 0) // поражение окончательное, концовка замерзания
        {
            player.playingMode = false;
            resultMode = false;
            nagradataker = false;
            mermanreader.skipEnemy();
            musica.finalka.volume = 0;
            musica.viseoplayer.SetActive(true);
            musica.cutscene2ringwoodfail.Play();

        }
        
    }

    public void pobedoid()
    {
        if (forLevel.languager == 0)
        {
            podtverjdenie.presentText.text = "Win";
        }
        if (forLevel.languager == 1)
        {
            podtverjdenie.presentText.text = "Победа";
        }
        else if (forLevel.languager == 2)
        {
            podtverjdenie.presentText.text = "Перемога";
        }
        if (forLevel.languager == 0)
        {
            podtverjdenie.presentText3.text = "Choose a reward";
        }
        if (forLevel.languager == 1)
        {
            podtverjdenie.presentText3.text = "Пора выбирать награду";
        }
        else if (forLevel.languager == 2)
        {
            podtverjdenie.presentText3.text = "Час обирати нагороду";
        }
        nagradataker = true;
        podtverjdenie.podtverjdenoTapom = false;
        podtverjdenie.presentBack.SetActive(true);
        Instantiate(ringvudit, place2.transform.position, Quaternion.identity, place2.transform);
        enemySpawner.dot1 = false;
        enemySpawner.dot2 = false;
        enemySpawner.dot3 = false;
    }

    public void Ringvushka()
    {
        battleModeVisibility.newMermaids = 0;
        forLevel.expe += 4;
        PlayerPrefs.SetFloat("expe", forLevel.expe);
        PlayerPrefs.SetFloat("level", forLevel.level);
        if (distanceCount.distance > forLevel.maxDepth)
        {
            PlayerPrefs.SetFloat("maxDepth", distanceCount.distance);
        }
        PlayerPrefs.Save();
        mermaids.mermaidButtonImitator.SetActive(false);
        presentIkra.SetActive(false);
        bonusikraText.text = " ";
        podtverjdenie.noteBack.SetActive(false);
        player.playingMode = false;
        resultMode = false;
        nagradataker = false;
        battleModeVisibility.skippingMode = true;
    }

}
