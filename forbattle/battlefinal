                            mermaids.herPosition6 = 44;
                            Instantiate(Khironda, mermaidslot6.transform.position, Quaternion.identity, mermaidslot6.transform);
                        }
                    }

                    if (holders.HadeeaMermaidKey)
                    {
                        PlayerPrefs.SetFloat("pHadeeaMermaid", 1);
                        PlayerPrefs.Save();
                        if (mermPosition1justset)
                        {
                            mermaids.herPosition1 = 45;
                            Instantiate(Hadeea, mermaidslot1.transform.position, Quaternion.identity, mermaidslot1.transform);
                        }
                        if (mermPosition2justset)
